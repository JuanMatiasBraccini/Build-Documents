@article{Fergusson2000,
	title = {Predation by {White} {Sharks} \&lt;i\&gt;{Carcharodon} carcharias\&lt;/i\&gt; ({Chondrichthyes}: {Lamnidae}) {Upon} {Chelonians}, with {New} {Records} from the {Mediterranean} {Sea} and a {First} {Record} of the {Ocean} {Sunfish} \&lt;i\&gt;{Mola} mola\&lt;/i\&gt; ({Osteichthyes}: {Molidae}) as {Sto}},
