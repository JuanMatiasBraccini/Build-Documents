	keywords = {fisheries, Fishery, Bycatch, Tuna, management, Fishing mortality, Mortality, Catch rate, Catch, Catch rates, Effort, Longline, Catch per unit effort, CPUE, temperature, Statistics, sea surface temperature, Pelagic longline},
