	author = {Patrick, Wesley S. and Spencer, Paul and Link, Jason and Cope, Jason and Field, John and Kobayashi, Donald and Lawson, Peter and Gedamke, Todd and Cortés, Enric and Ormseth, Olav and Bigelow, Keith and Overholtz, William},
