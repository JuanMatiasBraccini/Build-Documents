ISBN: 0044201532},
	keywords = {Broadnose sevengill sharks, Capture???recapture, CJS, JS, POPAN, Population estimation},
	pages = {761--772},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\P6847VUF\\2015-Integrating_acoustic_telemetry_into_mark-recapture_models_to_improve_the_precision_of_apparent_survival_and_abundan.pdf:application/pdf},
