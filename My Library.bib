	note = {ISSN: 00064971},
	keywords = {state-space models, recruitment, kalman filter, integrated analysis, mark-recapture-recovery data, multistate models, phalacrocorax carbo sinensis},
	pages = {917--946},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\JILWQJ8W\\m-api-3af9445f-b6aa-f2f3-1c50-4a1ef3e7642f.pdf:application/pdf},
