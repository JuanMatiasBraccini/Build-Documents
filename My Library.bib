@article{westArchivalTaggingSchool2001,
	title = {Archival {Tagging} {Of} {School} {Shark}, {Galeorhinus} {Galeus}, in {Australia}: {Initial} {Results}},
	volume = {60},
	issn = {0378-1909},
	url = {http://dx.doi.org/10.1023/A:1007697816642},
	abstract = {Archival tags were used to study the movement and depth behaviour of school sharks, Galeorhinus galeus, in southern australia. Thirty fish were tagged in late 1997, and to date there have been nine recaptures (30\% recapture rate). Periods at liberty varied from 8 days to 18 months. The sharks spent about 80\% of their time on the continental shelf, and appeared to swim close to the bottom during the day. At night they often ascended for periods of up to several hours, except at times around the full moon. When in deep water, the sharks typically descended at dawn to depths of up to 600?m, before ascending at dusk. It was not possible to use the light data from the tags to estimate position when the sharks were in deep water, because they were often at depths beyond the sensitivity of the tag. In shallower water, longitude was estimated from the light data but latitude was estimated from the maximum daily depth, assuming the fish were on the bottom. The timing of the dives in deepwater appeared sufficiently regular to offer the prospect of using it to estimate longitude. We propose future research using archival tags on this species should address questions about female reproductive migrations, pelagic behaviour and vertical movements},
	number = {1},
	journal = {Environmental Biology of Fishes},
	author = {West, Grant J and Stevens, John D},
	month = feb,
	year = {2001},
	note = {Publisher: Springer Netherlands},
	keywords = {shark, Behaviour, Biomedical and Life Sciences, movement, Fish, depth, archival tags, migration, Migrations, Archival tag},
	pages = {283--298},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\M6756RE8\\2001-Archival_Tagging_Of_School_Shark_Galeorhinus_Galeus_in_Australia_Initial_Results.pdf:application/pdf},
