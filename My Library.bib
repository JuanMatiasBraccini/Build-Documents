@article{Ebert2003,
	title = {Diet, feeding behaviour and habitat utilisation of the blue stingray \textit{{Dasyatis} chrysonota} ({Smith}, 1828) in {South} {African} waters},
	volume = {54},
	url = {http://www.publish.csiro.au/paper/MF03069},
	abstract = {Analysis of stomach contents for \textit{Dasyatis chrysonota} revealed that diet varied with size and habitat. The diet of all size classes in the surf zone was comprised primarily of \textit{Callianassa} spp., \textit{Donax} spp. and unidentified polychaete species. The medium and large size classes fed primarily on \textit{Donax} spp., whereas the very large size class fed mainly on \textit{Callianassa} spp. Polychaetes were of secondary importance as prey for the medium size class. The diet of \textit{D. chrysonota} in the nearshore zone consisted mainly of \textit{Balanoglossus capensis} and \textit{Callianassa} spp. \textit{Balanoglossus capensis} decreased from an index of relative importance (IRI) of 75.3\% for the medium size class to 59.9\% for the very large size class, whereas \textit{Callianassa} spp. increased from 22.8\% to 39.4\% between the medium and the very large size classes. The offshore zone was the only area in which small size class \textit{D. chrysonota} were caught. The diet of these small \textit{D. chrysonota} was primarily polychaetes and amphipods. Polychaetes increased in importance in the medium size class, but declined in each successively larger size class. Conversely, \textit{Pterygosquilla armata capensis} became the single most important prey item for the very large size class, comprising an IRI of 50.9\%. The behaviour pattern used by \textit{D. chrysonota} to locate and extract prey is described.{\textless}/p{\textgreater}},
	number = {8},
	journal = {Marine and Freshwater Research},
	author = {Ebert, David A and Cowley, Paul D},
	month = jan,
	year = {2003},
	keywords = {Behaviour, size, diet, habitat},
	pages = {957--965},
}

@article{STEVENS1986,
