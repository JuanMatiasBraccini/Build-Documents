	author = {Aires-da-Silva, A M and Gallucci, V F},
	month = jun,
	year = {2007},
	keywords = {shark, Growth, management, juveniles, blue shark, survival, demography, demography,shark fisheries management,Leslie matri, Leslie matri, shark fisheries management},
