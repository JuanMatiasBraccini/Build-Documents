	keywords = {shark, Behaviour, sharks, white shark, Tagging, movement, Pacific, Pelagic, carcharias, management, size, threatened, acoustic, predator, research, migration, Migrations, Carcharodon, conservation, genetics, monitoring, philopatry, predators, satellite tagging, threatened species},
	pages = {679--688},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\Q8AN9MFW\\2010-Philopatry_and_migration_of_Pacific_white_sharks.pdf:application/pdf},
