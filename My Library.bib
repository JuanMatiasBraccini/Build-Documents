	note = {Publisher: Blackwell Publishing Ltd},
	keywords = {shark, whale shark, sharks, reproduction, Age, size, Australia, Fish, survival, demography, Ningaloo Reef, Rhincodon typus, Leslie matrix model, mark–recapture, vital rates},
