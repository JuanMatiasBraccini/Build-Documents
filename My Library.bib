	pages = {386--9},
	file = {2011-Leadership_social_capital_and_incentives_promote_successful_fisheries..pdf:C\:\\Users\\myb\\Zotero\\storage\\EDL2NZCP\\2011-Leadership_social_capital_and_incentives_promote_successful_fisheries..pdf:application/pdf;2011-Leadership_social_capital_and_incentives_promote_successful_fisheries..pdf:C\:\\Users\\myb\\Zotero\\storage\\WFSYDEAM\\2011-Leadership_social_capital_and_incentives_promote_successful_fisheries..pdf:application/pdf},
