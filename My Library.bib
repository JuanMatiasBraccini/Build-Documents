	keywords = {Elasmobranchii, Animals, Conservation of Natural Resources, Elasmobranchii: physiology, Fisheries, Population Dynamics, Elasmobranchii: classification, Elasmobranchii: growth \& development, Human Activities},
	pages = {1075--119},
