	keywords = {shark, fisheries, Fishery, Bycatch, sharks, management, Carcharhinus, Decline, Catch, Effects of fishing, Effort, Fish, Catch per unit effort, CPUE, Exploitation, Reef shark, Coral reef fishery, Logbook},
	pages = {350--361},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\48QIRU34\\2009-Effects_of_fishing_on_tropical_reef_associated_shark_populations_on_the_Great_Barrier_Reef.pdf:application/pdf},
