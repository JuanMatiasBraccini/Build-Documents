	note = {Publisher: Springer Netherlands},
	keywords = {shark, Biomedical and Life Sciences, movement, habitat, migration, Migrations, temperature},
	pages = {225--250},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\J4NGSARW\\2001-Review_of_Elasmobranch_Behavioral_Studies_Using_Ultrasonic_Telemetry_with_Special_Reference_to_the_Lemon_Shark_Nega.pdf:application/pdf},
