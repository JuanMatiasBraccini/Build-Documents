	author = {Moura, T and Figueiredo, I and Farias, I and Serra-Pereira, B and Coelho, R and Erzini, K and Neves, A and Gordo, L S},
	month = dec,
	year = {2007},
	keywords = {reproduction, Age, Age and growth, Growth, Growth parameter, von Bertalanffy, maturity, growth,Portugal,reproduction,vertebrae., growth, Portugal, vertebrae.},
