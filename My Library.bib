	pages = {16--28},
	file = {Fu et al. - 2019 - Making ecological indicators management ready Ass.pdf:C\:\\Users\\myb\\Zotero\\storage\\8GCMQKLK\\Fu et al. - 2019 - Making ecological indicators management ready Ass.pdf:application/pdf},
}

