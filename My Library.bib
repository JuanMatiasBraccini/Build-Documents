	year = {2008},
	keywords = {shark, Behaviour, sharks, management, juvenile, Foraging behaviour, cetacean, carcass,cetacean,juvenile,scavenging,South Africa., carcass, scavenging, South Africa.},
	pages = {596--602},
}

