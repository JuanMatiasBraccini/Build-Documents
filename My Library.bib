	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\Z86IK3EM\\2015-Aquatic_animal_telemetry_A_panoramic_window_into_the_underwater_world..pdf:application/pdf},
}

@article{comeauAutomatedMonitoringLargescale2002,
	title = {Automated monitoring of a large-scale cod ({Gadus} morhua) migration in the open sea},
	volume = {59},
	issn = {0706-652X},
	url = {<Go to ISI>://000180811800001},
	doi = {10.1139/f02-152},
	abstract = {The migration patterns of marine fishes are poorly known, in part owing to the technical limitations associated with tracking the movements of animals in deep water. Here we document a large-scale, directed, migration of Atlantic cod (Gadus morhua) off eastern Canada. Our approach was based on the acoustic tagging of 126 fish and the deployment of 69 subsurface receivers, stretching over a 160-km distance along the edge of the Laurentian Channel. After 1 year of automated recording, we found that 65\% of the fish migrated out of coastal waters in two distinct runs during the summer-autumn period. The offshore-migrating fish overwintered in deep Laurentian Channel waters, returning inshore in April. Individual migration routes and migration timing were variable, indicating that the cod did not aggregate in large schools during the seasonal migration events.},
	number = {12},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Comeau, L A and Campana, S E and Castonguay, M},
	year = {2002},
	note = {ISBN: 0706-652X},
	keywords = {atlantic cod, gulf, st.-lawrence, stocks},
	pages = {1845--1850},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\BFD7VEP5\\2002-Automated_monitoring_of_a_large-scale_cod_(Gadus_morhua)_migration_in_the_open_sea.pdf:application/pdf},
}

