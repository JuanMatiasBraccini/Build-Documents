@article{Large1992,
	title = {Use of a multiplicative model to estimate relative abundance from commercial {CPUE} data},
	volume = {49},
	url = {http://icesjms.oxfordjournals.org/content/49/3/253.abstract},
	abstract = {A multiplicative model is developed for the CPUE of sole by UK beam trawlers fishing in the western English Channel between 1972 and 1982. The model includes a factor for vessel/vessel group as a means of adjusting for fishing power effects. Area, season, vessel and year are shown to have a significant effect on CPUE. Interactions between these main effects are investigated for a smaller model with grouped levels of each factor. Significant interactions are identified between the effects of year, season, vessel group and, to a lesser extent, area, but the variation in CPUE explained by each effect is small in comparison to the variation explained by main effects. Estimates of annual CPUE extracted from a main effects model (i.e. assuming no interactions) are shown to have a broadly similar trend to that observed in CPUE indices calculated using traditional rectangle and month averaging procedures. Some of the more obvious biases found in traditional indices are avoided. Approximately 60\% of the total observed variation in CPUE was not explained by the model, and it is suggested that analysis of CPUE data disaggregated by age may be appropriate},
	number = {3},
	journal = {ICES Journal of Marine Science: Journal du Conseil},
	author = {Large, P A},
	month = aug,
	year = {1992},
	keywords = {Abundance, Age, CPUE, Fishing power},
	pages = {253--262},
}

@incollection{cadrinAccountingSpatialPopulation2009,
	series = {Fish \& fisheries series},
	title = {Accounting for {Spatial} {Population} {Structure} in {Stock} {Assessment}: {Past}, {Present}, and {Future}},
	volume = {31},
	isbn = {978-1-4020-9210-7},
	url = {http://dx.doi.org/10.1007/978-1-4020-9210-7{_}22},
	abstract = {Stock identification has been an important prerequisite for stock assessment throughout its history. The earliest evaluations of recruitment variability recognized that understanding the spatial scale of a fishery resource is essential for studying population dynamics. A paradigm of stock structure was based on closed migration circuits and geographic variation of phenotypic traits and formed a premise for fishery modeling conventions in the mid-1900s. As genetic techniques developed in the late 1900s, the stock concept was refined to include a degree of reproductive isolation. Realization that there was no single method that addressed the various assumptions of stock assessment and needs of fishery management prompted a more holistic view of population structure that called for multiple sources of demographic and genetic data. Recent applications of advanced techniques challenge the traditional view of populations as geographically distinct units with homogeneous vital rates and isolation from adjacent resources. More complex concepts such as metapopulations and contingent theory may be more applicable to many fishery resources with sympatric population structure. These more complex patterns of population structure have been incorporated into some advanced stock assessment techniques and metapopulation models that account for movement among areas and sympatric heterogeneity. Wider application of spatially explicit models in future stock assessments will require clear identification of stock components, evaluating movement rates and determining the degree of reproductive isolation. Because spatial structure affects how populations respond to fisheries, incorporation of heterogeneous patterns and movement in stock assessment models should improve advice for fishery management},
	booktitle = {The {Future} of {Fisheries} {Science} in {North} {America}},
	publisher = {Springer Netherlands},
	author = {Cadrin, Steven X and Secor, David H},
	editor = {Beamish, Richard J and Rothschild, Brian J},
	year = {2009},
	note = {Series Title: Fish \&amp; Fisheries Series},
	keywords = {Fishery, movement, management, migration, Life Sciences},
	pages = {405--426},
}

