@article{Domi2005,
	title = {Feeding ecology of five commercial shark species of the {Celtic} {Sea} through stable isotope and trace metal analysis},
	volume = {60},
	issn = {0141-1136},
	url = {http://www.sciencedirect.com/science/article/B6V7H-4G94HN8-1/2/a88fa31cf7235f95f07069642345cfb2},
	abstract = {In order to trace their feeding habits, stable carbon and nitrogen isotope ratios ([delta]15N and [delta]13C), as well as trace metal concentrations (Zn, Cd, Fe, Cu, Se and Hg) were analysed in the tissues of five commercial shark species from the Celtic Sea: the tope shark Galeorhinus galeus, the black-mouthed catshark Galeus melastomus, the starry smooth hound Mustelus asterias, the spiny dogfish Squalus acanthias and the lesser-spotted dogfish Scyliorhinus canicula. Our results were compared to previously described stomach contents and isotopic composition of potential preys. Isotopic ratio [delta]15N suggested that tope sharks fed at a higher trophic level (16.7[per mille sign] in the muscle) than the other species, reflecting its piscivorous diet. The lower values of spiny dogfish (11.6[per mille sign] in the muscle) might be explained, amongst other things, by either its migratory behaviour or its preference for preys from lower trophic levels. Cd and Hg were correlated with isotopic ratios [delta]13C and [delta]15N, and were shown to be diet-related whereas Zn, Fe and Cu seemed much more linked to species-specific metabolism. Although this multidisciplinary approach is revealed as a useful tool for the study of shark ecology, the lack of known trophic fractionation suggests that isotopic data be compared to traditional diet analyses},
	number = {5},
	journal = {Marine Environmental Research},
	author = {Domi, N and Bouquegneau, J M and Das, K},
	month = dec,
	year = {2005},
	keywords = {elasmobranchs, shark, Behaviour, sharks, diet, Galeorhinus galeus, Scyliorhinus canicula, Squalus acanthias, Trophic level, Celtic Sea, Galeus melastomus, Mustelus asterias, Stable isotopes, Trace metals},
