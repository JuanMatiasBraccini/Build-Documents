	pages = {1395--1406},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\8N3GVXTD\\2011-Demographic_and_risk_analyses_of_spiny_dogfish_(Squalus_suckleyi)_in_the_Gulf_of_Alaska_using_age-_and_stage-based_.pdf:application/pdf},
