	year = {2010},
	keywords = {shark, ecosystem, sharks, management, Fish, snapper, cestode,fish health,hepatosomatic index,Lutjanidae, cestode, fish health, hepatosomatic index, Lutjanidae},
	pages = {163--169},
}

