	pages = {1--10},
	file = {PDF:C\:\\Users\\myb\\Zotero\\storage\\UBZJMVWY\\2010-Long-term_presence_and_movement_patterns_of_juvenile_bull_sharks_Carcharhinus_leucas_in_an_estuarine_river_system.pdf:application/pdf},
