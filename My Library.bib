	keywords = {shark, whale shark, sharks, Abundance, Australia, habitat, Biomass, temperature, linear model, sea surface temperature, aerial surveys,chlorophyll-,marine megafauna,Ninga, Ningaloo Reef, aerial surveys, chlorophyll-, marine megafauna, Ninga},
