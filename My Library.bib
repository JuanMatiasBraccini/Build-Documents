@article{henningsenReviewLevelsRecirculating1998,
	title = {Review. {Levels} of {Recirculating} {Reproductively}-related {Steroid} {Hormones} in {Female} {Elasmobranchs}. {Implications} for {Reproduction} in a {Captive} {Environment}},
	volume = {2},
	issn = {1357-5325},
	url = {http://dx.doi.org/10.1023/A:1009688826898},
	abstract = {Chondrichthyans are a diverse group in terms of reproductive mode, which range from extended oviparity to placental viviparity. Although the reproductive biology of several species has been investigated, the reproductive physiology of only a few species has been reported. Only recently have the physiological factors of reproduction in cartilaginous fishes received attention. The information gained on reproductive cycles (both gonadal and uterine) has shown some similarities as well as differences between species. The serum levels of androgens, oestrogens, and progestins have been examined in several elasmobranch species and it has been shown that they are important in regulating key events in reproduction. The endocrine control of reproduction in elasmobranchs is more similar in many ways to higher vertebrates such as mammals than to teleosts. In general, the role of progesterone becomes more important than that of oestrogen in the transition from oviparity to viviparity. In addition, the roles of peptide hormones such as relaxin and the neurohypophysial hormones become increasingly important in viviparous species as in higher vertebrates. It is vital to understand the environmental and hormonal control of reproduction in elasmobranchs if more success is to be achieved in their reproduction in captivity. Knowledge gained from studies of the endocrine regulation of the reproductive cycle can be used to enhance the reproductive success of captive sharks, skates, and rays},
	number = {3},
	journal = {Aquarium Sciences and Conservation},
	author = {Henningsen, Alan},
	month = sep,
	year = {1998},
	note = {Publisher: Springer Netherlands},
	keywords = {Elasmobranch, elasmobranchs, shark, sharks, reproduction, Biomedical and Life Sciences, reproductive, Chondrichthyan, chondrichthyans, biology, Environment, rays, physiology, reproductive physiology, captive, Skate, skates, serum, Cartilaginous fishes, fishes, viviparity, general, oviparity},
	pages = {97--116},
}

