	journal = {Ecological Indicators},
	author = {Fu, Caihong and Xu, Yi and Bundy, Alida and Grüss, Arnaud and Coll, Marta and Heymans, Johanna J. and Fulton, Elizabeth A. and Shannon, Lynne and Halouani, Ghassen and Velez, Laure and Akoglu, Ekin and Lynam, Christopher P. and Shin, Yunne-Jai},
	month = oct,
