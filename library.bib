@article{bracciniQuantifyingSharkRay2021,
  title = {Quantifying Shark and Ray Discards in {{Western Australia}}’s Shark Fisheries},
  author = {Braccini, Matias and Murua, Hilario},
  date = {2021-11-09},
  journaltitle = {Marine and Freshwater Research},
  shortjournal = {Mar. Freshwater Res.},
  volume = {73},
