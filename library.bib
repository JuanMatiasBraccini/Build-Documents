  author = {Taylor, Ian G. and Doering, Kathryn L. and Johnson, Kelli F. and Wetzel, Chantel R. and Stewart, Ian J.},
  date = {2021-07},
  journaltitle = {Fisheries Research},
  shortjournal = {Fisheries Research},
  volume = {239},
