@article{tillettSimilarLifeHistory2011,
  title = {Similar Life History Traits in Bull ({{Carcharhinus}} Leucas) and Pig-Eye ({{C}}. Amboinensis) Sharks},
  author = {Tillett, Bree J. and Meekan, Mark G. and Field, Iain C. and Hua, Quan and Bradshaw, Corey J.A.},
  date = {2011},
  journaltitle = {Marine and Freshwater Research},
