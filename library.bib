@article{dureuilUnifiedNaturalMortality2021,
  title = {Unified Natural Mortality Estimation for Teleosts and Elasmobranchs},
  author = {Dureuil, M and Aeberhard, Wh and Burnett, Ka and Hueter, Re and Tyminski, Jp and Worm, B},
